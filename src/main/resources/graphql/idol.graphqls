schema {
    query: Query
}

type Query {
#    selectIdolGroupList(id: Int, groupNm: String): [IdolGroupLimit]
    selectIdolInfo(idolNm: String!): IdolMembers
    selectIdolGroupMembers(id: Int, groupNm: String): [IdolGroup]
}

type IdolGroupLimit {
    id: Int
    groupNm: String
    agency: String
}

type IdolGroup {
    id: Int
    groupNm: String
    agency: String
    idolMembers: [IdolMembers]
}

type IdolMembers{
    id: Int
    idolNm: String
    birth: String
}